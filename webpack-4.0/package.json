{
  "name": "react-webpack-typescript-babel",
  "version": "1.0.0",
  "description": "A sample setup using React, Webpack and Babel to build TypeScript code, and using TypeScript for type-checking.",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "cross-env CLIENT_ENV=production webpack --mode=production",
    "start": "cross-env CLIENT_ENV=development webpack-dev-server --mode=development --open"
  },
  "author": {
    "name": "Alexander T.",
    "email": "alexander.tarasyuk@outlook.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "7.5.5",
    "@babel/preset-env": "7.5.5",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.3.3",
    "babel-loader": "8.0.6",
    "cross-env": "5.2.0",
    "css-loader": "3.2.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "fork-ts-checker-webpack-plugin": "1.5.0",
    "html-webpack-plugin": "3.2.0",
    "node-sass": "4.12.0",
    "react-svg-loader": "3.0.3",
    "sass-loader": "7.3.1",
    "style-loader": "1.0.0",
    "typescript": "3.5.3",
    "webpack": "4.39.2",
    "webpack-cli": "3.3.7",
    "webpack-dev-server": "3.8.0"
  },
  "dependencies": {
    "@types/jest": "24.0.18",
    "@types/node": "12.7.2",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "react": "16.9.0",
    "react-dom": "16.9.0"
  },
  "resolutions": {
    "terser": "3.14.1"
  }
}
