{
  "name": "react-webpack-typescript-babel",
  "version": "1.0.0",
  "description": "A sample setup using React, Webpack and Babel to build TypeScript code, and using TypeScript for type-checking.",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "cross-env CLIENT_ENV=production webpack --mode=production",
    "start": "cross-env CLIENT_ENV=development webpack-dev-server --mode=development --open"
  },
  "author": {
    "name": "Alexander T.",
    "email": "alexander.tarasyuk@outlook.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.7.7",
    "@babel/plugin-proposal-class-properties": "7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "7.7.7",
    "@babel/preset-env": "7.7.7",
    "@babel/preset-react": "7.7.4",
    "@babel/preset-typescript": "7.7.7",
    "babel-loader": "8.0.6",
    "cross-env": "6.0.3",
    "css-loader": "3.4.1",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "fork-ts-checker-webpack-plugin": "3.1.1",
    "html-webpack-plugin": "3.2.0",
    "node-sass": "4.13.0",
    "react-svg-loader": "3.0.3",
    "sass-loader": "8.0.0",
    "style-loader": "1.1.2",
    "typescript": "3.7.4",
    "webpack": "4.41.5",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.10.1"
  },
  "dependencies": {
    "@types/jest": "24.0.25",
    "@types/node": "13.1.4",
    "@types/react": "16.9.17",
    "@types/react-dom": "16.9.4",
    "react": "16.12.0",
    "react-dom": "16.12.0"
  },
  "resolutions": {
    "terser": "^4.1.2"
  }
}
